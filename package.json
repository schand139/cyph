{
  "name": "cypher-blockchain-analytics",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "api": "node api-server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run api\"",
    "build": "tsc && vite build",
    "build:api": "tsc -p tsconfig.api.json",
    "build:all": "npm run build && npm run build:api",
    "lint": "eslint . --ext ts,tsx,js --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preload-cache": "node scripts/preloadCache.js",
    "update-cache-alchemy": "node scripts/updateCacheWithAlchemy.js",
    "vercel-build": "npm run build:all && npm run preload-cache"
  },
  "dependencies": {
    "@vercel/blob": "^1.0.2",
    "@vercel/kv": "^0.2.4",
    "alchemy-sdk": "^3.5.9",
    "autoprefixer": "^10.4.17",
    "chart.js": "^4.4.1",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "viem": "^2.7.9"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
